            tools:ignore="UnusedAttribute" />
        <activity android:name=".calendar.ViewMealsActivity" />
        <activity
            android:name=".SignUpActivity"
            android:parentActivityName=".LoginActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".tools.GroceryListActivity"
            android:parentActivityName=".HomePageActivity"
            tools:ignore="UnusedAttribute" />
        <activity android:name=".test.SpringTestActivity" />
        <activity android:name=".personal.SetLimitsActivity"
            android:parentActivityName=".HomePageActivity"
            tools:ignore="UnusedAttribute" />
    </application>

</manifest>