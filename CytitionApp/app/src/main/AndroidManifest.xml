        <activity
            android:name=".food.RecipeActivity"
            android:label="Recipes"
            android:parentActivityName=".HomePageActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".food.IngredientActivity"
            android:label="Ingredients"
            android:parentActivityName=".food.SingleRecipeActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".food.AddRecipeActivity"
            android:label="Add Recipes"
            android:parentActivityName=".food.RecipeActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".food.SingleRecipeActivity"
            android:label="Recipe"
            android:parentActivityName=".food.RecipeActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".food.AddIngredientActivity"
            android:label="Add Ingredients"
            android:parentActivityName=".food.SingleRecipeActivity"
            tools:ignore="UnusedAttribute" />
        <activity
            android:name=".family.AddChildActivity"
