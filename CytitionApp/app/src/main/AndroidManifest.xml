            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".tools.CalculatorActivity"
            android:parentActivityName=".HomePageActivity" />
        <activity
            android:name=".calendar.CalendarActivity"
            android:parentActivityName=".HomePageActivity"
